{
  "name": "esm-fake-loader",
  "version": "1.1.4",
  "author": "Phil Toms <phil.toms@hotmail.co.uk>",
  "repository": {
    "type": "git",
    "url": "git://github.com/philtoms/esm-fake-loader.git"
  },
  "main": "index.mjs",
  "license": "MIT",
  "scripts": {
    "test": "node --loader ./index --experimental-specifier-resolution=node ./node_modules/.bin/ava --verbose"
  },
  "devDependencies": {
    "ava": "^3.8.2"
  },
  "ava": {
    "nodeArguments": [
      "--no-warnings"
    ],
    "files": [
      "**/*.test.mjs"
    ]
  },
  "keywords": [
    "esm",
    "es modules",
    "import",
    "dynamic import",
    "export",
    "require",
    "inject",
    "test",
    "mock",
    "stub",
    "spy"
  ],
  "bugs": {
    "url": "https://github.com/philtoms/esm-fake-loader/issues"
  },
  "homepage": "https://github.com/philtoms/esm-fake-loader"
}
